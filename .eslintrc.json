{
  "extends": ["next/core-web-vitals", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "env": {
    "es2021": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "no-unused-vars": "warn",
    "eqeqeq": ["error", "always"],
    "no-console": "off"
  }
}
